<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="StarBankProyecto.Views.Admin_cuentas">
    <ContentPage.Content>
        <ScrollView>
            <StackLayout>
                <Label Text="Bienvenido"/>

                <StackLayout>
                    <Frame>
                        <Button x:Name="saldos" Text="Saldo" Clicked="saldos_Clicked"/>
                    </Frame>

                    <Frame>
                        <Button x:Name="historial" Text="Historial" Clicked="historial_Clicked"/>
                    </Frame>

                    <Frame>
                        <Button x:Name="transc" Text="Transacciones" Clicked="transc_Clicked"/>
                    </Frame>
                </StackLayout>
            </StackLayout>
        </ScrollView>
    </ContentPage.Content>
</ContentPage>