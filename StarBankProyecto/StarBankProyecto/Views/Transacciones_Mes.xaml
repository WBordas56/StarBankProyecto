        <StackLayout>

            <Label Text="Transacciones del Mes" TextColor="#0f0f0f" FontFamily="{StaticResource PoppinsRegular}" Margin="5,5,5,0" Padding="15,20,15,0" FontSize="16"/>

            <Entry Keyboard="Text" Background="White" Margin="20,0,20,0" FontFamily="{StaticResource PoppinsRegular}" FontSize="12" Placeholder="Buscar" PlaceholderColor="#c1c1c1"/> 
            <Frame Margin="20,0,20,0" HeightRequest="1000">
            
            </Frame>

        </StackLayout>
    </ScrollView>

</ContentPage>